{
  "package.json": {
    "name": "movedid-ai-guardian",
    "version": "0.1.0",
    "description": "AI-powered decentralized identity protection system",
    "main": "src/index.tsx",
    "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "test": "jest",
      "move:compile": "movement move compile",
      "move:test": "movement move test"
    },
    "keywords": [
      "blockchain",
      "ai",
      "security",
      "movedid",
      "movement-network"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
      "@aptos-labs/ts-sdk": "^1.3.0",
      "@aptos-labs/wallet-adapter-react": "^1.0.0",
      "@fleek-platform/ai-sdk": "^0.1.0",
      "next": "^14.0.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "tailwindcss": "^3.3.0",
      "typescript": "^5.0.0"
    },
    "devDependencies": {
      "@types/jest": "^29.5.0",
      "@types/react": "^18.0.0",
      "eslint": "^8.0.0",
      "jest": "^29.5.0",
      "move-cli": "^0.1.0",
      "prettier": "^2.8.0"
    }
  },
  ".gitignore": [
    "node_modules/",
    ".next/",
    "out/",
    "build/",
    ".env.local",
    ".env.development.local",
    ".env.test.local",
    ".env.production.local",
    "*.log",
    ".DS_Store"
  ],
  ".env.example": {
    "MOVEMENT_NETWORK_ENDPOINT": "https://aptos.testnet.porto.movementlabs.xyz/v1",
    "FAUCET_ENDPOINT": "https://fund.testnet.porto.movementlabs.xyz/",
    "AI_MODEL_API_KEY": "",
    "NEXT_PUBLIC_FLEEK_API_KEY": ""
  },
  "tsconfig.json": {
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
}
